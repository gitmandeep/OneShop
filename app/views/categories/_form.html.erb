<%= render 'category_error' %>
<%= bootstrap_form_with scope: :category, url: categories_path, method: :POST, remote: true do |form| %>
  
    <%= form.text_field :name, class: "cat-text" %>
 
    <%= form.submit class: "btn btn-primary btn-xm" %> <%= link_to 'Back', products_url,class: "btn btn-warning btn-xm" %>
  
<% end %>

