<div class="col-sm-6">
<h2>Sign up</h2>
<%= bootstrap_form_for(resource, as: resource_name, url: registration_path(resource_name),label_errors: true, inline_errors: true) do |f| %>
  <%#= devise_error_messages! %> 
  <%= f.text_field :first_name %>   
  <%= f.text_field :last_name %>   
  <%= f.date_field :dob %> 
  <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  <%= f.password_field :password, autocomplete: "new-password",help: "Must be at least 6 characters long" %>
  <%= f.password_field :password_confirmation, autocomplete: "new-password" %>
     <%= f.text_field :mobile %>
  <%= f.submit "Sign up", class: "btn btn-primary btn-xm" %>  <%= link_to "Back", "/users/sign_in",class: "btn btn-warning btn-xm" %>
<% end %>
</div>
